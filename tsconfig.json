{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "lib": ["es6", "dom", "es2017"],
    "target": "es5",
    "jsx": "react-jsx",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "sourceMap": true
  },
  "exclude": ["node_modules"],
  "include": ["./src/**/*", "node_modules/snb-components/**/*"]
}